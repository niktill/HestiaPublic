{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true,
    "node": true,
    "jest/globals": true
  },
  "extends": ["airbnb", "prettier", "plugin:node/recommended"],
  "parserOptions": {
    "ecmaVersion": 12
  },
  "plugins": ["prettier"],
  "rules": {
    "consistent-return": "off",
    "prefer-arrow-callback": "warn",
    "no-unused-vars": "warn"
  }
}
